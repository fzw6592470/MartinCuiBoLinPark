.text-center{
	text-align: center;
	height: 96rpx;
	line-height: 96rpx;
}
.code-list{
	width:100%;
	height: 96rpx;
	line-height: 96rpx;
	position: relative;
	overflow: hidden;
	padding-left: 32rpx;
	padding-right: 32rpx;
	box-sizing: border-box;
}
.code-list .code-item{
	float:left;
	box-sizing: border-box;
	width: 16.667%;
	height: 96rpx;
	line-height: 96rpx;
	text-align: center;
	border: 2rpx solid #e2e2e2;
	border-right: 0 none;
	background: #fff;
	font-size: 24rpx;
}
.code-item.c-active{
	border: 2rpx solid #56bd64;
}
.code-item:first-child{
	border-top-left-radius: 10rpx;
	border-bottom-left-radius: 10rpx;
}
.code-item:last-child{
	border-right: 1px solid #e2e2e2;
	border-top-right-radius: 10rpx;
	border-bottom-right-radius: 10rpx;
}
.code-item:last-child.c-active{
	border: 1px solid #56bd64;
}
